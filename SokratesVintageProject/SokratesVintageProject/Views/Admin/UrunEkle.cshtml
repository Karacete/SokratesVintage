@model SokratesVintageProject.Models.UrunModel
@{
    ViewData["Title"] = "Urun Ekle";
}
<form class="form-group" asp-controller="Admin" asp-action="UrunEkle"method="post" enctype="multipart/form-data">
    <div class="form-outline mb-4">
        <label class="form-label" for="form2Example18">Ürün Adı:</label>
        <input type="text" id="form2Example18" class="form-control form-control-lg" asp-for="UrunAd" />
    </div>

    <div class="form-outline mb-4">
        <label class="form-label" for="form2Example28">Ürün Kategori:</label>
        <input type="number" id="form2Example28" class="form-control form-control-lg" asp-for="UrunKategori" />
    </div>
    <div class="form-outline mb-4">
        <label class="form-label" for="form2Example28">Ürün Fiyatı:</label>
        <input type="number" id="form2Example28" class="form-control form-control-lg" asp-for="UrunFiyat" />
    </div>
    <div class="form-outline mb-4">
        <label class="form-label" for="form2Example28">Ürün Fotoğrafı:</label>
        <input type="file" id="form2Example28" class="form-control form-control-lg" asp-for="UrunFotograf" />
    </div>
    <div class="pt-1 mb-4">
        <button class="btn btn-primary">Kaydet</button>
    </div>
</form>

